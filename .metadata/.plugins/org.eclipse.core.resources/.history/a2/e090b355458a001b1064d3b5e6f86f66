#include "filo.h"
#include <iostream>

using namespace std;

int main(){
	FILO que;

	cout << que.ptr->a << endl;
	que.put(10);
	cout << que.ptr->a << endl;
	que.put(12);
	cout << que.ptr->a << endl;
	que.put(14);
	cout << que.ptr->a << endl;

	cout << que.get() <<endl;
	cout << que.get() <<endl;
	cout << que.get() <<endl;

}
